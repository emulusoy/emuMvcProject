@model emuPortfolio.Models.Entity.TblSocialMedia
@{
    ViewBag.Title = "UpdateSocialMediaIcon";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<h2>Update Skills</h2>


<form method="post">

    <input type="hidden" name="SocialMediaID" value="@Model.SocialMediaID" />
        <input type="hidden" name="Status" value="@Model.Status" />
        @Html.Label("Social Media Name")
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
        <br />
        @Html.Label("Social Media Link")
        @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
        <br />
        @Html.Label("Social Media Icon")
        @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
        <br />
        <button class="btn btn-success">Update Icons</button>


    </form>

